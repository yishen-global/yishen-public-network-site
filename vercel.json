{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    { "handle": "filesystem" },
    { "src": "^/api/(.*)$", "dest": "/api/$1" },
    { "src": "^/nexus/ca/joolan/?$", "dest": "/public/nexus/joolan/ca/index.html" },
    { "src": "^/nexus/us/ca/joolan/?$", "dest": "/public/nexus/joolan/us/ca/index.html" },
    { "src": "^/nexus/us/tx/joolan/?$", "dest": "/public/nexus/joolan/us/tx/index.html" },
    { "src": "^/nexus/us/fl/joolan/?$", "dest": "/public/nexus/joolan/us/fl/index.html" },
    { "src": "^/nexus/us/ny/joolan/?$", "dest": "/public/nexus/joolan/us/ny/index.html" },
    { "src": "^/nexus/us/az/joolan/?$", "dest": "/public/nexus/joolan/us/az/index.html" },
    { "src": "^/(.*)$", "dest": "/index.html" }
  ]
}
